{
  "manifest_version": 2,
  "name": "jira-brsower",
  "description": "chrome extension",
  "version": "0.0.1",
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "action": {
    "default_icon": {
      "16": "./64jira.png",
      "32": "./64jira.png",
      "48": "./64jira.png",
      "128": "./64jira.png"
    }
  },
  "icons": {
    "16": "./64jira.png",
    "32": "./64jira.png",
    "48": "./64jira.png",
    "128": "./64jira.png"
  },
  "permissions": [
    "storage"
  ]
}
